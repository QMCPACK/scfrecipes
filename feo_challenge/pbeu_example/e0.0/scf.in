&CONTROL
   calculation     = 'scf'
   disk_io         = 'low'
   outdir          = 'pwscf_output'
   prefix          = 'pwscf'
   pseudo_dir      = '../../pseudos'
   tprnfor         = .false.
   tstress         = .false.
